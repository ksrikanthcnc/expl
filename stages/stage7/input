type
node{
	int val;
	node next;
}

linkedlist{
	int data;
	linkedlist next;
}

marklist{
	str name;
	linkedlist marks;
	marklist next;
}

endtype

decl
	marklist mlist,temp;
enddecl

int main(){
	decl
		node x,y,z;
		int i;
	enddecl
	
	begin
		initialize();
		
		mlist=alloc();
		mlist.name="one";
		mlist.marks=alloc();
		write(mlist.marks);
		mlist.marks.data=45;
		
		mlist.next=alloc();
		mlist.next.name="two";
		mlist.next.marks=alloc();
		mlist.next.marks.data=40;
		
		write(mlist.marks.data);
		write(mlist.next.marks.data);
		if(mlist.marks.data>mlist.next.marks.data)then
			write("two thope");
		else
			print "one thope";
		endif;
		
		return 0;
	end
}
